{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=aquamind.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "AquaMind": {
    "Settings": {
      "DefaultIrrigationTime": "00:15:00",
      "MinHumidity": 30.0,
      "MaxHumidity": 80.0,
      "AlertEmailEnabled": true,
      "AlertSMSEnabled": false
    }
  }
}
